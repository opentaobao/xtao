<!DOCTYPE HTML>
<html>
	<head>
		<meta charset="utf-8" />
		<title>{{=it.title}}</title>
		<link rel="stylesheet" type="text/css" href="http://a.tbcdn.cn/apps/top/g/css/bootstrap.min.css" />
		<link rel="stylesheet" type="text/css" href="http://localhost/top-assets/g/css/prettify.min.css">
		<link rel="stylesheet" type="text/css" href="http://a.tbcdn.cn/apps/top/g/css/racoon-min.css">
		<style type="text/css">
			{{=it.css}}
		</style>
	</head>
	<body>
		<div id="page" class="sdk-demo full-height">
			<div class="row-fluid full-height">
				<div class="span12 full-height">
					{{=it.html}}
				</div>
			</div>
		</div>
		<script type="text/javascript" src="http://a.tbcdn.cn/apps/top/g/js/jquery.min.js"></script>
		<script type="text/javascript" src="http://a.tbcdn.cn/apps/top/g/js/bootstrap.min.js"></script>
		<script type="text/javascript" src="http://a.tbcdn.cn/apps/top/g/js/2.5.3-crypto-md5-hmac.js"></script>
		<script type="text/javascript" src="http://a.tbcdn.cn/apps/top/g/js/2.5.3-crypto-sha1-hmac.js"></script>
		<script type="text/javascript" src="http://a.tbcdn.cn/apps/top/g/js/2.5.3-crypto-sha256-hmac.js"></script>
		<script type="text/javascript" src="{{=it.sdkUrl}}"></script>
		<script type="text/javascript">
			;(function(win, undefined){
				var appKey = '{{=it.key}}'
					, appSecret = '{{=it.secret}}'
					, appHostname = '{{=it.hostname}}'
					, channelUrl = '{{=it.channelUrl}}'
					, doc = win.document
					, timestamp = new Date().getTime()
					, message = [
												appSecret
											, 'app_key'
											, appKey
											, 'timestamp'
											, timestamp
											, appSecret
											].join('')
					, hmac = Crypto.HMAC(Crypto.MD5, message, appSecret).toUpperCase();

					document.cookie = 'timestamp=' + timestamp;
					document.cookie = 'sign=' + hmac;

					//init
					TOP.init({
						appKey: appKey
					, status: false
					, channelUrl: window.location.protocol + "//" + window.location.hostname + channelUrl
					});
			})(window);
		</script>
		<script type="text/javascript" src="http://localhost/top-assets/g/js/prettify.min.js"></script>
		<script type="text/javascript" src="http://a.tbcdn.cn/apps/top/g/js/racoon-min.js"></script>
		<script type="text/javascript">
			{{=it.js}}
		</script>
	</body>
</html>